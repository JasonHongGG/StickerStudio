# Base URL used by frontend API client (also used as dev proxy target for /api)
VITE_API_URL=http://localhost:3001

# Frontend dev server port (used by docker-compose)
VITE_PORT=5173

# Comma-separated hostnames allowed by Vite dev server
# Example: VITE_ALLOWED_HOSTS=stickstudio.alberthongtunnel.dpdns.org
VITE_ALLOWED_HOSTS=
